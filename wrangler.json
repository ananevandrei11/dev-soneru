{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "dev-soneru",
  "main": "./workers/app.ts",
  "compatibility_date": "2025-04-01",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "vars": {
    "VALUE_FROM_CLOUDFLARE": "Hello from Cloudflare"
  },
  "env": {
    "production": {
      "name": "soneru",
      "workers_dev": true
    },
    "development": {
      "name": "soneru-dev",
      "workers_dev": true
    }
  }
}
